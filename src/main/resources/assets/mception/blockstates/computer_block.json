{
	"multipart": [
		{   "when": { "AND": [{"active": "false", "facing": "north"}]},
			"apply": { "model": "mception:block/computer_off" }
		},
		{   "when": { "AND": [{"active": "false", "facing": "east"}]},
			"apply": { "model": "mception:block/computer_off", "y":  90 }
		},
		{   "when": { "AND": [{"active": "false", "facing": "south"}]},
			"apply": { "model": "mception:block/computer_off", "y": 180 }
		},
		{   "when": { "AND": [{"active": "false", "facing": "west"}]},
			"apply": { "model": "mception:block/computer_off", "y": 270 }
		},

		{   "when": { "AND": [{"active": "true", "facing": "north"}]},
			"apply": { "model": "mception:block/computer_on" }
		},
		{   "when": { "AND": [{"active": "true", "facing": "east"}]},
			"apply": { "model": "mception:block/computer_on", "y":  90 }
		},
		{   "when": { "AND": [{"active": "true", "facing": "south"}]},
			"apply": { "model": "mception:block/computer_on", "y": 180 }
		},
		{   "when": { "AND": [{"active": "true", "facing": "west"}]},
			"apply": { "model": "mception:block/computer_on", "y": 270 }
		}
	]
}